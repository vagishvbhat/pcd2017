#include<stdio.h> 
+struct complex
+{
+int real,img;
+};
+int main()
+{
+struct complex a,b,c;
+printf("Enter a and b where a + ib is the first complex number.\n");
+printf("a=");
+scanf("%d", &b.real);
+printf("d=");
+scanf('%d", &b.img);
+c.real=a.real+b.real;
+c.img=a.img+b.img;
+if(c.img>=0)
+printf("sum of two complex numbers =%d + %di\n",c.real,c.img);
+else
+printf("sum of two complex numbers= %d %di\n",c.real,c.img);
+return 0;
+}
